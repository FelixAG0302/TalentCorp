@model TalentCorp.Entities.Entrevista

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>Entrevista</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Fecha
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaEntrevista)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Candidato)
        </dt>
        <dd class = "col-sm-10">
